pipeline {
    environment {
        PATH="${getmaven()}:${PATH}"
    }
    stages{
        stage("Git Checkout") {
            steps{
                git 'https://github.com/kalyanp89/myweb.git'
            }
        }
        
    }
}


// 
def getmaven(){
    def mvnhome = tool name: 'maven3', type: 'maven'
    return "${mvnhome}/bin"
}